{
  "name": "vimms-downloader-ui",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron . --dev\"",
    "dev:all": "concurrently \"ts-node-esm src/server/app.ts\" \"vite\" \"wait-on http://localhost:5173 && electron . --dev\"",
    "start:api": "ts-node-esm src/server/app.ts",
    "run:cli": "ts-node-esm src/cli/runVimms.ts",
    "build": "vite build && electron-builder",
    "preview": "vite preview",
    "lint": "echo \"no lint configured\"",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/supertest": "^2.0.12",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.2.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "electron": "^26.0.0",
    "electron-builder": "^24.6.0",
    "express": "^4.18.2",
    "extract-zip": "^2.0.1",
    "node-html-parser": "^6.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsx": "^4.21.0"
  },
  "build": {
    "appId": "com.example.vimmsdownloader",
    "files": [
      "dist/**",
      "electron/**",
      "package.json"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}